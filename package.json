{
	"name": "newsroom-system",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"format": "prettier --write \"src/**/*.{js,ts,svelte,css,html}\" \"scripts/**/*.js\" \"config/**/*.js\" \"*.{json,md}\" \"svelte.config.js\" \"tailwind.config.js\" \"vite.config.js\"",
		"lint": "prettier --check \"src/**/*.{js,ts,svelte,css,html}\" \"scripts/**/*.js\" \"config/**/*.js\" \"*.{json,md}\" \"svelte.config.js\" \"tailwind.config.js\" \"vite.config.js\"",
		"// === üé® VISUAL BUILDER ===": "",
		"visual-builder": "vite dev --mode builder",
		"builder": "vite dev --mode builder --open /builder",
		"builder:viewer": "vite dev --mode builder --open /builder/viewer",
		"builder:deploy": "node scripts/builder-deploy.js",
		"// === üöÄ WORKFLOW PRINCIPAL ===": "",
		"start": "node scripts/workflow.js",
		"workflow": "node scripts/workflow.js",
		"workflow:update": "node scripts/workflow.js update",
		"workflow:config": "node scripts/workflow.js config",
		"// === üîß FIX PARA EMBED S3 ===": "",
		"fix-urls": "node scripts/fix-absolute-urls.js",
		"build-embed": "npm run build && npm run fix-urls",
		"export:build": "node scripts/export-build.js",
		"// === üìπ FRAMES ===": "",
		"extract": "node scripts/extract-frames.js",
		"extract:all": "node scripts/extract-frames.js --process-all",
		"extract:list": "node scripts/extract-frames.js --list-videos",
		"// === üßπ CACHE MANAGEMENT ===": "",
		"cache:complete": "node scripts/enhanced-cache-generator.js && ./warm-cache.sh",
		"cache:generate": "node scripts/auto-cache-generator.js",
		"cache:warm": "./warm-cache.sh",
		"cache:batch": "./warm-cache-batch.sh",
		"cache:all": "npm run cache:generate && npm run cache:warm",
		"cache:purge": "node scripts/cache-purge.js",
		"cache:purge:project": "node scripts/cache-purge.js project",
		"cache:purge:script": "node scripts/cache-purge.js script",
		"cache:clear": "npm run cache:purge",
		"cache:check": "node scripts/check-cache-status.js",
		"cache:check:file": "node scripts/check-cache-status.js --file",
		"// === üìÑ GOOGLE DOCS ===": "",
		"fetch": "node scripts/fetch-docs.js",
		"// === ‚òÅÔ∏è UPLOAD/DEPLOY ===": "",
		"upload": "node scripts/upload-globo-storage.js",
		"deploy": "node scripts/smart-deploy.js",
		"// === üé• V√çDEOS ===": "",
		"download-video": "node scripts/video-downloader.js"
	},
	"devDependencies": {
		"@storybook/addon-controls": "^9.0.8",
		"@storybook/svelte-vite": "^9.1.6",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"autoprefixer": "^10.4.21",
		"daisyui": "^4.12.24",
		"lucide-svelte": "^0.344.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"tailwindcss": "^3.4.17",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@neodrag/svelte": "^2.3.3",
		"@sveltejs/svelte-scroller": "^2.0.7",
		"archieml": "^0.5.0",
		"axios": "^1.11.0",
		"dotenv": "^17.2.1"
	},
	"optionalDependencies": {
		"fluent-ffmpeg": "^2.1.3",
		"form-data": "^4.0.4",
		"node-fetch": "^3.3.2"
	}
}
